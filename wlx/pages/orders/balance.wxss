page{
    background-color:#f0f0f0;
}
.ad_popError{ 
    background: #de352d; 
    color: #fff; height: 58rpx; 
    line-height: 58rpx; 
    font-size: 24rpx; 
    text-align: center; 
    position: absolute; 
    left: 0; 
    top: 0; 
    width: 100%; 
    z-index:3;
    }
.ad_popHd{
    height: 76rpx; 
    line-height: 76rpx;
    font-size: 32rpx;
    text-align: center;
    border-bottom: 1px solid #cdd1cd;
    padding: 0 20rpx;
    color: #202120;
   } 
.container{
    background-color: #fff;
    padding-bottom:24rpx;
    }
.main1{
    position: relative;
    top:0;
    left: 0;
}
.msg{
    margin-left: 86rpx;
}
.list1{
    font-size:24rpx;
    color:#333;
    line-height:70rpx;
    padding-top: 15rpx;
}
.list1 text:nth-child(2){
    margin-left:30rpx;
}
.list2{
    font-size:28rpx;
    color:#333;
    margin-bottom: 10rpx;
    display: flex;
    flex-direction: column;
}
.location{
    width:34rpx;
    height: 43rpx;
    position: absolute;
    top:50rpx;
    left: 26rpx;
}
.rt_btn{
    width:17rpx;
    height: 29rpx;
    position: absolute;
    top:65rpx;
    right: 30rpx;
}
.btm_line{
    width: 739rpx;
    height: 11rpx;
}
.line{
    background-color:#f0f0f0;
    height: 20rpx;  
    border-top:2px solid #e5e5e5;
    border-bottom:2px solid #e5e5e5;
}
.time{
    display: flex;
    align-items: center;
    line-height: 86rpx;
    border-bottom:1px solid #e5e5e5;
    padding:0 30rpx;
}
.time_img{
    width:39rpx;
    height: 39rpx;
    margin-right: 17rpx;
}
.time_btn{
    width: 17rpx;
    height: 29rpx;
    margin-left: auto;
}
.time text{
    color:#333;
    font-size: 28rpx;
}
.log__hd{
    display: flex;
    align-items: center;
    background-color: #ffffff;
    height: 70rpx;
    border-bottom:1px solid #dcdcdc;
    padding:0 22rpx;
}
.log__icon{
    width: 32rpx;
    height: 32rpx;
}
.log__shop-name{
    font-size:28rpx;
    color:#333;
}
.log__cost{
    color: #aaa;
    margin-left: auto;
    font-size: 22rpx;
}
.log__bd{
    display: flex;
    align-items: flex-end;
    height: 140rpx;
    padding:0 30rpx;
}
.log__image{
    height: 114rpx;
    width: 114rpx;
    border-radius: 5rpx;
}
.order{
    display: flex;
    flex-direction: column;
    margin-left: 24rpx;
    justify-content:space-around;
    height:120rpx;
    width:80%;
}
.order__number{
    font-size:28rpx;
    color: #333;
}
.order__datetime{
    font-size: 36rpx;
    color: #ec6a2c;
}
.order__count{
    font-size:24rpx;
    color:#aaa;
    float: right;
}
.log{
    padding-bottom: 20rpx;
}
.integral{
    margin-left:20rpx;
    line-height: 87rpx;
    padding-right:30rpx;
    display: flex;
    align-items: center;
    border-bottom: 1px solid #e5e5e5;
}
.intergal_img{
    width: 40rpx;
    height: 40rpx;
    margin-right: 20rpx;
}
.integral text{
    font-size:28rpx;
    color:#333;
}
.integral text.on{
    color:#ec6a2c;
    margin-left: 12rpx;
}
.money{
    margin-left: auto;
}
.pay_way{
    margin-left:20rpx;
    line-height: 98rpx;
    padding-right:30rpx;
    display: flex;
    align-items: center;
    border-bottom: 1px solid #e5e5e5;
    font-size:28rpx;
    color:#333;
}
.pay_way .right{
    margin-left: auto;
}

.pay_way image{
    width: 17rpx;
    height: 18rpx;
    margin-left: 17rpx;
}
.remark{
    font-size:28rpx;
    color:#aaa;
    margin:22rpx 30rpx 0 20rpx;
    border:1px solid #cccccc;
    border-radius: 10rpx;
    padding-left:11rpx;
    height: 72rpx;
}
.foot{
    background-color:#fff;
    position:absolute;
    bottom:0;
    left:0;
    height:111rpx;
    width: 90%;
    display: flex;
    align-items: center;
    padding:0 5%;
}
.foot .sale{
    font-size:20rpx;
    color:#aaa;
    margin-right:40rpx;
}
.foot .paid{
    font-size:30rpx;
    color:#333;
}
.foot .paid .on{
    font-size:36rpx;
    color:#ec6a2c;
    margin-left:12rpx;
}
.present{
    background-color:#0085e0;
    width: 215rpx;
    height:85rpx;
    color:#fff;
    font-size:30rpx;
    margin-left: auto;
    margin-right: 0;
}
